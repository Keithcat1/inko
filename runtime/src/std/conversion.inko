#! Traits for runtime type conversion.

trait ToFloat {
  ## Converts `self` to a `Float`.
  def to_float -> Float
}

trait ToString {
  ## Converts `self` to a `String`.
  def to_string -> String
}

trait ToInteger {
  ## Converts `self` to an `Integer`.
  def to_integer -> Integer
}

trait ToArray!(T) {
  ## Converts `self` to an `Array`
  def to_array -> Array!(T)
}
